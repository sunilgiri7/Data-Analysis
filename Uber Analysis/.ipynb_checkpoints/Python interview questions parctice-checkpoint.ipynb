    "def fibonacci(n):\n",
    "    a,b = 0,1\n",
    "    while a<=n:\n",
    "        yield a\n",
    "        a, b = b, a+b\n",
    "n = int(input(\"Enter the last range of fibonacci\"))\n",
    "a = fibonacci(n)\n",
    "for num in a:\n",
    "    print(num)"
